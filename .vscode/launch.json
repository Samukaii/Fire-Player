  "version": "0.2.0",
  "configurations": [
      
    {
      "name": "Debug Android",
      "cwd": "${workspaceFolder}",
      "type": "reactnative",
      "request": "launch",
      "platform": "android"
    },
    {
      "name": "Attach to packager",
      "cwd": "${workspaceFolder}",
      "type": "reactnative",
      "request": "attach"
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Executar Programa",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\start"
    }
  ]
}
